---
import Master from './master/Master.astro';
import Teasernav from './components/Teasernav.astro';
import Sidenav from './components/Sidenav.astro';
const { frontmatter } = Astro.props;
---

<Master frontmatter={frontmatter}>
   <slot />
   <Teasernav
      headingsAs={'h2'}
      ariaLabel={`${frontmatter.title} submenu`}
      iconFile="/images/docs-component-navicons.svg"
      frontmatter={frontmatter}
   />
   <Sidenav slot="sidenav" frontmatter={frontmatter} />
</Master>
